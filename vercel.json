{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "nextjs",
  "env": {
    "NODE_OPTIONS": "--max_old_space_size=4096",
    "NEXT_PUBLIC_APP_URL": "https://syndicate-lens.vercel.app",
    "NEXT_PUBLIC_ENVIRONMENT": "production",
    "NEXT_PUBLIC_LENS_MAINNET_RPC_URL": "https://rpc.lens.xyz",
    "NEXT_PUBLIC_LENS_TESTNET_RPC_URL": "https://rpc.testnet.lens.xyz",
    "NEXT_PUBLIC_BASE_CHAIN_RPC_URL": "https://mainnet.base.org"
  }
}